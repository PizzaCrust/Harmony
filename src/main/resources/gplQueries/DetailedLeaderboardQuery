query PlaygroundsApp_playground_Query(  $playgroundSlug: String!) {  me {    ...PlaygroundsApp_me    id  }  playground(slug: $playgroundSlug) {    ...PlaygroundsApp_playground    id  }}fragment PlaygroundsApp_me on User {  ...PlaygroundsEventsManager_me  ...PlaygroundsSidebar_me  ...PlaygroundsMainContent_me  ...PlaygroundsMembers_me}fragment PlaygroundsApp_playground on Playground {  id  name  description  slug  amIKicked  amIBanned  ...PlaygroundsEventsManager_playground  ...PlaygroundsSidebar_playground  ...PlaygroundsMainContent_playground  ...PlaygroundsMembers_playground}fragment PlaygroundsEventsManager_playground on Playground {  id  slug  modActionChannelName}fragment PlaygroundsSidebar_playground on Playground {  ...TextChannelsAndLobbies_playground  ...PlaygroundPickerDropdown_playground}fragment PlaygroundsMainContent_playground on Playground {  textChannels {    id    name    canJoin    pubnubMessageChannelName    pubnubSignalChannelName  }  ...PlaygroundsChat_playground  ...PlaygroundsMainContentHeader_playground}fragment PlaygroundsMembers_playground on Playground {  id  slug  membersCount  ...MembersList_playground}fragment MembersList_playground on Playground {  id  slug  myPermissions  creator {    id    slug    username    avatarUrl    socialLink    socialTwitter    socialDiscord    socialMixer    socialTwitch    socialYoutube  }  membersCount  roles {    nodes {      id      name      color    }  }}fragment PlaygroundsChat_playground on Playground {  id  slug  name  amIMuted  amIKicked  amIBanned  myPermissions  myPrimaryRole {    color    id  }  textChannels {    id    name    canJoin    canIRead    canIWrite    canRateLimit    rateLimitMs    pubnubMessageChannelName  }}fragment PlaygroundsMainContentHeader_playground on Playground {  slug  textChannels {    id    name    canJoin    pubnubMessageChannelName    pubnubSignalChannelName  }}fragment TextChannelsAndLobbies_playground on Playground {  id  slug  myPermissions  textChannels {    id    name    canJoin    pubnubMessageChannelName    pubnubSignalChannelName  }  playgroundLobbies {    nodes {      id      slug      name      canJoin      pubnubSignalChannelName      currentLobbyLeaderboard {        id        slug        config        participantsCount      }    }  }  ...CreateTextChannelDialog_playground}fragment PlaygroundPickerDropdown_playground on Playground {  id  name  slug  membersCount  creator {    slug    id  }  myPermissions  ...InvitePeopleDialog_playground}fragment InvitePeopleDialog_playground on Playground {  id  name  slug  roles {    nodes {      id      name    }  }}fragment CreateTextChannelDialog_playground on Playground {  id  slug  roles {    nodes {      id      name      color    }  }  textChannels {    id    canJoin    pubnubMessageChannelName    pubnubSignalChannelName  }}fragment PlaygroundsEventsManager_me on User {  id  slug}fragment PlaygroundsSidebar_me on User {  ...TextChannelsAndLobbies_me  ...PlaygroundPickerDropdown_me}fragment PlaygroundsMainContent_me on User {  id  ...PlaygroundsChat_me}fragment PlaygroundsMembers_me on User {  id  ...MembersList_me}fragment MembersList_me on User {  id  slug}fragment PlaygroundsChat_me on User {  id  slug  username  avatarUrl}fragment TextChannelsAndLobbies_me on User {  slug  isVisorAdmin  fflippers(features: ["useLobbyParty"])}fragment PlaygroundPickerDropdown_me on User {  slug  isVisorAdmin  ...InvitePeopleDialog_me}fragment InvitePeopleDialog_me on User {  slug}